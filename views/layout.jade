doctype html
html
  head
    meta(charset='utf8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
    meta(name='description', content='Simple project templating and skeletons')
    meta(name='author', content="Carrot Creative")
    meta(name='viewport' content='width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1')
    title Sprout
    script(src="//use.typekit.net/ydj7swv.js")
    script.
      try{Typekit.load();}catch(e){}
    link(rel='stylesheet', href='http://cdnjs.cloudflare.com/ajax/libs/highlight.js/8.2/styles/default.min.css')
    != css()
  body

    header
      h1 Sprout
      h2 simple project templating &amp; skeletons

    .contain
      nav
        .logo-small: h2 Sprout
        ul
          li(data-id='installation'): a(href='#installation') Installation
          li(data-id='usage'): a(href='#usage') CLI Usage
          li(data-id='js-api'): a(href='#js-api') JS API
          li(data-id='writing'): a(href='#js-api') Writing Templates
          li(data-id='versioning'): a(href='#versioning') Versioning 
          
      main
        block content

  script(src='http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js')
  != js()
